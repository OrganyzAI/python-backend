# Domain
# This would be set to the production domain with an env var on deployment
DOMAIN=localhost

# Environment: local, staging, production
ENVIRONMENT=local

# Backend
BACKEND_CORS_ORIGINS="http://localhost:3000,http://localhost:3001"
DOCKER_IMAGE_BACKEND=backend

# Project name
PROJECT_NAME="Full Stack FastAPI Project"

# Configure these with your own Docker registry images
DOCKER_IMAGE_BACKEND=backend
DOCKER_IMAGE_FRONTEND=frontend

# Backend
BACKEND_CORS_ORIGINS="http://localhost,http://localhost:3000,http://localhost:8080"

# Emails
SMTP_HOST=
SMTP_USER=
SMTP_PASSWORD=
EMAILS_FROM_EMAIL=info@example.com
SMTP_TLS=True
SMTP_SSL=False
SMTP_PORT=587

# Postgres
POSTGRES_SERVER=localhost
POSTGRES_USER=app
POSTGRES_PASSWORD=changethis123
POSTGRES_DB=app
POSTGRES_PORT=5432

# Sentry
SENTRY_DSN=

# Superuser
FIRST_SUPERUSER=admin@example.com
FIRST_SUPERUSER_PASSWORD=changethis123

# OpenRouter API
# Get your API key from https://openrouter.ai/api/v1
OPENROUTER_API_KEY=sk-or-v1-YOUR_OPENROUTER_API_KEY_HERE
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# WebEngage API
WEBENGAGE_API_KEY=YOUR_WEBENGAGE_API_KEY_HERE
